generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id        String   @id @default(uuid())
  username  String   @unique
  nombre    String
  password  String
  rol       String   @default("usuario")
  createdAt DateTime @default(now())
}